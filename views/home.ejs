<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Desafio Fullstack</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/flexboxgrid.css">
    <link rel="stylesheet" href="/css/base.css">
</head>
<body>
    
    <div class="nav">
        <input type="checkbox" id="nav-check">
        <div class="nav-header">
          <div class="nav-title">
            Agrotools
          </div>
        </div>
        <div class="nav-btn">
          <label for="nav-check">
            <span></span>
            <span></span>
            <span></span>
          </label>
        </div>
        
        <div class="nav-links">
          <a href="/">Home</a>
          <a href="/novo">Novo Questionário</a>
        </div>
    </div>

    <div class="container-fluid">
        <h3>Questionários</h3>
        <div class="row">
            <% questionarios.forEach(function(questionario) { %>
                <div class="col-xs-12 shadow-sm bg-white my-1">
                    <h4><%= questionario.titulo %></h4>
                </div>
            <% }) %>
        </div>
    </div>

</body>
</html>